{
  "private": true,
  "name": "nest-rest-template",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "gcp-build": "npm run build",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:gcp": "functions-framework --target=main",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/index.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivkos/nest-rest-template.git"
  },
  "author": {
    "name": "Ivaylo Stoyanov",
    "email": "me@ivkos.com",
    "url": "https://github.com/ivkos"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ivkos/nest-rest-template/issues"
  },
  "homepage": "https://github.com/ivkos/nest-rest-template#readme",
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "axios": "^0.19.2",
    "lodash": "^4.17.15",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.5.1",
    "@google-cloud/storage": "^5.0.1",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.67",
    "@types/supertest": "^2.0.8",
    "@types/yaml": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "firebase-admin": "^8.12.1",
    "jest": "26.4.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
